{
  "COMPOSER_VENDOR_DIR": "{BUILD_DIR}/vendor",
  "WEBDIR": "public",
  "LIBDIR": "./",
  "PHP_VERSION": "{PHP_70_LATEST}",
  "PHP_EXTENSIONS": [
    "pdo",
    "pdo_mysql",
    "redis",
    "bz2",
    "zlib",
    "curl",
    "mcrypt",
    "mbstring",
    "json",
    "tokenizer",
    "spl",
    "reflection",
    "dom",
    "gd",
    "zip"
  ],
  "COMPOSER_INSTALL_OPTIONS": [
    "--no-interaction",
    "--no-dev",
    "--no-progress",
    "--prefer-dist"
  ]
}
